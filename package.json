{
  "name": "tovly-portfolio",
  "description": "Software Engineer & Harvard Student",
  "license": "MIT",
  "scripts": {
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby develop -o",
    "develop": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby develop",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier --write src/sites/sites.yaml",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@types/react": "^16.9.36",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.32.13",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-netlify": "^2.1.34",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-plugin-styled-components": "^3.1.20",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-source-filesystem": "^2.1.50",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.3.17",
    "gatsby-transformer-yaml": "^2.2.26",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-id-swiper": "^1.6.9",
    "styled-components": "^4.4.1",
    "tailwindcss": "^1.4.6",
    "twin.macro": "^1.4.0",
    "typeface-montserrat": "git+https://github.com/TovlyDeutsch/personal-typefaces-montserrat.git"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.2",
    "@graphql-codegen/typescript": "^1.15.2",
    "@graphql-codegen/typescript-operations": "^1.15.2",
    "babel-eslint": "^10.1.0",
    "caniuse-api": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "prettier": "^1.15.3"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "browserslist": [
    ">.25% and not dead and not IE 11 and not op_mini all and last 3 versions"
  ]
}
